<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="750.0" prefWidth="750.0" spacing="20.0" style="-fx-background-color: #353535;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.main.librarymanagementsystem.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label prefHeight="80.0" prefWidth="183.0" text="Добро пожаловать!" textAlignment="CENTER" textFill="#ab5a9e">
        <font>
            <Font size="20.0" />
        </font>
    </Label>

    <Button mnemonicParsing="false" onAction="#onStartButtonClick" style="-fx-background-color: ab5a9e;" text="Запустить приложение">
        <font>
            <Font size="15.0" />
        </font>
    </Button>
</VBox>
